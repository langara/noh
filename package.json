{
  "name": "noh",
  "version": "1.0.0",
  "description": "Prehistoric NO HTML library",
  "main": "noh.js",
  "scripts": {
    "check": "google-closure-compiler --checks_only --process_common_js_modules --module_resolution NODE --warning_level VERBOSE --externs ./src/jquery-3.3-externs.js --js ./src/noh_tests.js package.json",
    "build": "webpack",
    "start": "webpack-dev-server --entry ./src/noh_tests.js --output-filename ./dist/noh_tests_dev.js --open-page ./noh_tests.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langara/noh.git"
  },
  "keywords": [
    "no",
    "html",
    "js",
    "dom",
    "jquery",
    "browser",
    "prehistoric"
  ],
  "author": "Marek Langiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langara/noh/issues"
  },
  "homepage": "https://github.com/langara/noh#readme",
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "css-loader": "^2.1.0",
    "google-closure-compiler": "^20190121.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
